	for (int i = 0; i < Q; i++) {
		int signal;
		scanf ("%d", &signal);
		if (signal == 1) {
			int left, right, moidfy;
			scanf ("%d %d %d", &left, &right, &moidfy);
			Modify(left, right, modify);
			
		}else {
			int left, int right;
			scanf ("%d %d", &left, &right);
